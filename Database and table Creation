CREATE TABLE Professors (
    professor_id   VARCHAR(50) PRIMARY KEY,
    professor_name VARCHAR(50),
    professor_email VARCHAR(100),
    department_id INT,
    FOREIGN KEY (department_id) REFERENCES Departments(department_id)
);



